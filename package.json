{
  "name": "id-hasher",
  "version": "2.0.1",
  "description": "IdHasher is a library that generates short, unique, non-sequential ids from numbers.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "coverage": "jest --coverage",
    "prepublishOnly": "tsc"
  },
  "repository": "https://github.com/EJayCheng/hash-id",
  "author": "EJay Cheng",
  "license": "MIT",
  "dependencies": {
    "sqids": "^0.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "jest": "^29.7.0",
    "ts-jest": "^24.3.0",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
